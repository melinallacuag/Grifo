<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FacturaFragment">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="CARA"
        android:textColor="#3C4048"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="MANGUERA"
        android:textColor="#3C4048"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.052"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="MODALIDAD"
        android:textColor="#3C4048"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20sp"
        android:text="OPERACIÃ“N"
        android:textColor="#3C4048"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnlibre" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24sp"
        android:text="TRANSACCIONES"
        android:textColor="#3C4048"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnserafi" />

    <Button
        android:id="@+id/btnlibre"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="24sp"
        android:layout_marginTop="20sp"
        android:letterSpacing="0.05"
        android:text="LIBRE"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btnsoles"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:letterSpacing="0.05"
        android:text="SOLES"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toEndOf="@+id/btnlibre"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btngalones"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:letterSpacing="0.05"
        android:text="GALONES"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toEndOf="@+id/btnsoles"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/btnboleta"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="20sp"
        android:letterSpacing="0.05"
        android:text="BOLETA"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnfactura"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:letterSpacing="0.05"
        android:text="FACTURA"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toEndOf="@+id/btnboleta"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnfac"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="20dp"
        android:letterSpacing="0.05"
        android:text="FAC."
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnfactura"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <Button
        android:id="@+id/btnserafi"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="10sp"
        android:letterSpacing="0.05"
        android:text="SERAFI"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnboleta" />

    <Button
        android:id="@+id/btncanje"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="16sp"
        android:layout_marginTop="10sp"
        android:letterSpacing="0.05"
        android:text="CANJE"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toEndOf="@+id/btnserafi"
        app:layout_constraintTop_toBottomOf="@+id/btnfactura" />

    <Button
        android:id="@+id/btnncredito"
        android:layout_width="110sp"
        android:layout_height="50sp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="10sp"
        android:letterSpacing="0.05"
        android:text="NOTA CREDITO"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:cornerRadius="20sp"
        app:layout_constraintStart_toEndOf="@+id/btncanje"
        app:layout_constraintTop_toBottomOf="@+id/btnfac" />
</androidx.constraintlayout.widget.ConstraintLayout>